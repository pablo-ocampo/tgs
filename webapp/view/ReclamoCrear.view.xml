<mvc:View
	controllerName="neo.tgs.controller.ReclamoCrear"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page class="sapUiResponsiveContentPadding" >
		<customHeader>
			<Bar>
				<contentLeft>
					<Title class="lilaText" text="Creación de Reclamos" titleStyle="H2"/>
				</contentLeft>
			</Bar>
		</customHeader>
		<subHeader>
			<OverflowToolbar>
				<Breadcrumbs currentLocationText="Crear" separatorStyle="GreaterThan">
					<Link press="onPress" text="Inicio"/>
					<Link press="onPress" text="Reclamos"/>
				</Breadcrumbs>
			</OverflowToolbar>
		</subHeader>
		<content>
			<Text text="Un simple texto que indica que estás en la creación de reclamos"/>
		</content>
	</Page>
</mvc:View>